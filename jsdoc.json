{
    "plugins": [],
    "recurseDepth": 10,
    "source": {
        "include": ["pages/", "preload/"],
        "exclude": [".cli.js","main.js"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "opts": {
        "destination": "./documentation/",
        "recurse": true
    }
}

